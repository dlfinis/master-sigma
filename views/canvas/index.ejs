  <div class="container">
    <div class="row">

                <% _.each(articles, function (article) { %>
                  <div class="col-lg-12 text-center post">
                          <img class="img-responsive img-full"
                          src= "<%=(article.image)? article.image:'images/submarine.png'%>"
                          alt="">
                          <div class="title"><h3><%=article.title%></h3></div>
                          <div class="box">
                            <h5><%=article.updatedAt%></h5>
                            <p><%=article.description%></p>
                            <a href="<%=article.url%>" class="btn btn-default btn-lg">Leer m√°s</a>
                           </div>
                      <div class="social-bar">
                            <div class="section-btn">
                              <a class="btn"><img src="icons/thumb.png" /></a>
                            <a href="#" class="btn btn-lg"><img src="icons/share.png" /></a>
                            </div>
                      </div>
                  </div>
                <% }) %>

            <div class="col-lg-12 text-center">
                <ul class="pager">
                    <li class="previous"><a href="#">&larr; Older</a>
                    </li>
                    <li class="next"><a href="#">Newer &rarr;</a>
                    </li>
                </ul>
            </div>

    </div>

  </div>
    <!-- /.container -->
